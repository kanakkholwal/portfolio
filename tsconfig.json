{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "src/*": [
        "./src/*"
      ],
      "layouts/*": [
        "./src/layouts/*"
      ],
      "hooks/*": [
        "./src/hooks/*"
      ],
      "models/*": [
        "./src/models/*"
      ],
      "lib/*": [
        "./src/lib/*"
      ],
      "types/*": [
        "./src/types/*"
      ],
      "components/*": [
        "./src/components/*"
      ],
      "utils/*": [
        "./src/utils/*"
      ],
      "middleware/*": [
        "./src/middleware/*"
      ],
      "assets/*": [
        "./src/assets/*"
      ],
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src",
    ".storybook/StoryContainer.js",
    "pages"
  ],
  "exclude": [
    "node_modules"
  ]
}